<core:FragmentDefinition xmlns:m="sap.m" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:forms="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc">
    <m:VBox id="vboxBasicInfo" class="sapUiSmallMargin">
        <sap.ui.layout.form:SimpleForm
            id="_IDGenSimpleForm1"
            xmlns:sap.ui.layout.form="sap.ui.layout.form"
            xmlns:sap.ui.core="sap.ui.core"
            class="sapUxAPObjectPageSubSectionAlignContent"
            editable="false"
            layout="ColumnLayout"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsM="2"
            columnsL="3"
            columnsXL="4"
            singleContainerFullSize="false"
        >
            <sap.ui.layout.form:content>
                <m:Label id="_IDGenLabel9" text="{i18n>sfCategory}" />
                <m:Text id="_IDGenText5" text="{to_cat/Description}" class="sapMObjectNumberMedium" />

                <m:Label id="_IDGenLabel10" text="{i18n>sfSupplier}" />
                <m:Text id="_IDGenText6" text="{Supplierid} - {Suppliername}" class="sapMObjectNumberMedium" />
            </sap.ui.layout.form:content>
        </sap.ui.layout.form:SimpleForm>
    </m:VBox>
</core:FragmentDefinition>
